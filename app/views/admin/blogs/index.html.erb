<h1>投稿一覧</h1>
<table class ="table">
  <thread>
    <tr>
      <th>タイトル</th>
      <th>カテゴリ</th>
      <th>更新日時</th>
    </tr>
  </thread>


  <tbody>
  <% @blogs.each do |blog| %>
  <tr>
    <td class="info col-xs-1">
      <%= link_to blog.title, admin_blog_path(blog.id) %>
    </td>
    <td class="text-left">
      <%= blog.category %>
    </td>
    <td class="text-left">
      <%= blog.created_at%>
    </td>
  </tr>

  <% end %>
  </tbody>

</table>
 <%= link_to "新規作成", new_admin_blog_path, class:"btn btn btn-outline-secondary" %>



