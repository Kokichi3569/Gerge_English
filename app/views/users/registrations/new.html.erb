<h3>新規会員登録</h3>

<%= form_for(resource, as: resource_name, url: registration_path(resource_name)) do |f| %>
  <%= render "users/shared/error_messages", resource: resource %>

  <th>名前</th>
  <td class="sign-up-small-cell">（性）</td>
  <td><%= f.text_field :last_name, autofocus: true %></td>
  <td class="sign-up-small-cell">（名）</td>
  <td><%= f.text_field :first_name, autofocus: true %></td>
  <th>フリガナ</th>
  <td class="sign-up-small-cell">（セイ）</td>
  <td><%= f.text_field :last_name_kana, autofocus: true %></td>
  <td class="sign-up-small-cell">（メイ）</td>
  <td><%= f.text_field :first_name_kana, autofocus: true %></td>
  <td class="sign-up-small-cell">メールアドレス</td>
  <td><%= f.email_field :email, autofocus: true %></td>
  <td class="sign-up-small-cell">電話番号（ハイフンなし）</td>
  <td><%= f.text_field :tel_number, autofocus: true %></td>
  <td class="sign-up-small-cell">パスワード(6文字以上)</td>
  <td><%= f.password_field :password %></td>
    <td class="sign-up-small-cell">パスワード（確認用）</td>
  <td><%= f.password_field :password_confirmation, autocomplete: "current-password" %></td>

  <div class="actions">
    <%= f.submit "新規登録", class:"btn btn-succsess col-xs-offset-5" %>
  </div>
<% end %>

<%= render "users/shared/links" %>
